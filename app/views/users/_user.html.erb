<% if flash[:message] %>
  <p><%= flash[:message] %></p>
<% end %>
<ul>
  <li>
    Name: <%= user.name %>
  </li>
  <li>
    Mood: <%= user.mood  %>
  </li>
  <li>
    Tickets: <%= user.tickets %>
  </li>
  <li>
    Height: <%= user.height %>
  </li>
  <li>
    <% if user.admin %>
    ADMIN
    <% end %>
  </li>
</ul>
<%= link_to "See attractions", attractions_path %>
<%= link_to "Log Out", logout_path, method: :post %>